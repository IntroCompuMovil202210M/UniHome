<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    tools:context=".PrincipalArrendatario">


    <fragment
        android:id="@+id/mapA"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="9"/>

    <LinearLayout
        android:id="@+id/barraA"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_weight="1"
        android:background="@color/ColorLogoUnihome"
        android:orientation="horizontal">


        <ImageButton
            android:id="@+id/cuentaA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            android:background="@color/ColorLogoUnihome"
            android:src="@drawable/cuentapic" />

        <ImageButton
            android:id="@+id/chatsA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            android:background="@color/ColorLogoUnihome"
            android:src="@drawable/chatspic" />

        <ImageButton
            android:id="@+id/publicarA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            android:background="@color/ColorLogoUnihome"
            android:src="@drawable/publicarpic" />
    </LinearLayout>

</LinearLayout>
